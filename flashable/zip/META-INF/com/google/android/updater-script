ui_print("");
ui_print("-- Extracting Modules");
set_progress(1.000000);
delete_recursive("/system/lib/modules");
package_extract_dir("system", "/system");
package_extract_dir("kernel", "/tmp");
ui_print("Installing kernel...");
set_perm(0, 0, 0777, "/tmp/mkbootimg.sh");
set_perm(0, 0, 0777, "/tmp/mkbootimg");
run_program("/tmp/mkbootimg.sh", file_getprop("/system/build.prop", "ro.build.version.release"));
run_program("/sbin/busybox", "rm", "-rf", "/sdcard/WildKernel-boot.img");
run_program("/sbin/busybox", "cp", "/tmp/boot.img", "/sdcard/WildKernel-boot.img");
write_raw_image("/tmp/boot.img", "boot");
run_program("/sbin/busybox", "umount", "/system");
ui_print("-- Done");
